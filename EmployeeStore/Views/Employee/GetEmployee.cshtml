@model EmployeeStore.Models.Employee

@{
    ViewBag.Title = "GetEmployee";
}

<h2>Employee Details</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>
                FirstName
            </th>
            <th>LastName</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @Model.FirstName
            </td>
            <td>
                @Model.LastName
            </td>
            <td>
                @Model.Gender
            </td>
        </tr>
    </tbody>
</table>